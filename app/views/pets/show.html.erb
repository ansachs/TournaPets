<div class ='container'>
  <img src=<%= @pet.profile_pic %> />
  <h1> <%= @pet.name %> </h1>
  <p> <%= @pet.breed %> </p>
  <p> <%= link_to 'Back', pets_path, class: 'btn btn-primary' %> </p>
</div>

<div class='container'>
  <h3> All of <%= @pet.name %>'s Photos </h3>
  <div class='row'>
    <div class='col-s'>
      <% @pet.media_links.each do |obj| %>
        <img style="height:200px" src= <%= obj.link %> />
      <% end %>
    </div>
  </div>
</div>
