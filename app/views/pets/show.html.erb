<div class ='container'>
  <img src=<%= @pet.profile_pic %> />
  <h1> <%= @pet.name %> </h1>
  <p>Breed: <%= @pet.breed %></p></center>
  <%= logged link_to 'Edit', edit_pet_path(@pet), class: 'btn btn-primary' %> |
